<%= render 'features/navbar'%>
<section class="container">
    <%= render 'shared/error_messages' %>
    <h4>Edit Project</h4>
    <%= form_for [@user,@project] do |project| %>
        <%= project.label "Project Title" %>
        <%= project.text_field :projectname, placeholder: "project title..." ,class: "form-control" %><br>
        <%= project.button :submit, :class => "btn btn-primary"%>
    <%end%>    
</section>